public class CommandObj {
	
    public String type = "";
	public String key = "";
	public String name = "";
	public String server = "";
	public String element = "";
	public String method = "";
	public String seq = "";
	public String sqlId = "";
	
	public CommandObj( String type, String key, String name, String server, String group, String method, String seq, String sqlId ){
		
	    this.type = type;
		this.key = key;
		this.name = name;
		this.server = server;
		this.element = group;
		this.method = method;
		this.seq = seq;
		this.sqlId = sqlId;
	}
	
	public String toString(){
		return "[type:"+type+"][key:"+key+"][server:"+server+"][element:"+element+"][name:"+name+"][method:"+method+"][seq:"+seq+"][sql_id:"+sqlId+"]";
	}
}
