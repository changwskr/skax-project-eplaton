<?xml version="1.0" encoding="euc-kr"?>
<!--
	ServiceObjects.xml Sample
-->
<!DOCTYPE ServiceObjects [
	<!--
		* CAUTION *
		FrameworkObjects는 framework에서만 사용된다.
		업무팀에서 재정의해도 loading되지 않는다
	-->
	<!ELEMENT FrameworkObjects (providers*, frameworkObject*)>
	<!--
		업무팀에서는 ServiceObjects를 사용한다.
	-->
	<!ELEMENT ServiceObjects (providers*, object*)>
	<!ELEMENT providers (provider+)>
	<!ATTLIST providers
		type (JNDI | ClassLoader) #REQUIRED
		default CDATA #REQUIRED
	>
	<!ELEMENT provider (param*)>
	<!ATTLIST provider
		name CDATA #REQUIRED
	>
	<!ELEMENT param (#PCDATA)>
	<!ATTLIST param
		name CDATA #REQUIRED
		value CDATA #REQUIRED
	>
	<!ELEMENT frameworkObject (param*)>
	<!ATTLIST frameworkObject
		name CDATA #REQUIRED
		type (JNDI | ClassLoader) #REQUIRED
		provider CDATA #IMPLIED
		singleton (true | false) "false"
	>
	<!ELEMENT object (param*)>
	<!ATTLIST object
		name CDATA #REQUIRED
		type (JNDI | ClassLoader) #REQUIRED
		provider CDATA #IMPLIED
	>
]>

<ServiceObjects>
	<!-- Async Error Manager Preference
		Async 처리시 업무별로 특정 Error 처리 Manager를 가져야할 경우, 등록한다.
		등록되는 Async Error Manager는 
		com.kbstar.ksa.oltp.async.impl.IAsyncErrorProcess 를 상속해서 구현한 class 여야한다.
	-->
	<!-- 
	<object name="EDU_AsyncErrorProcess" type="ClassLoader"
		provider="basic">
		<param name="Name"
			value="com.kbstar.ksa.sr.impl.async.AsyncErrorProcess" />
	</object>
	 -->

	<!-- 업무 pre/post process 세팅 필요한 경우 합니다. -->
	
	<object name="PreProcess" type="ClassLoader" provider="basic">
	    <param name="Name" value="com.kbstar.kji.oltp.transaction.impl.PreProcess" />
	</object>
	<object name="PostProcess" type="ClassLoader" provider="basic">
	    <param name="Name" value="com.kbstar.kji.oltp.transaction.impl.PostProcess" />
	</object>
	 
</ServiceObjects>
