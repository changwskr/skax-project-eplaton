<?xml version="1.0" encoding="UTF-8"?>
<!-- 
BZCRUDBUS WAF 명령어 설정
생성일: 2024년
설명: BZCRUDBUS 프레임워크의 WAF(Web Application Framework) 명령어 설정

id: 호출할 id(화면id+eventid)
key: 해당 Method 실행후 Return된 객체를 request.setAttribute 시킬때 키값
element: 결과 그룹의 자식(결과 데이터) Element 명
name: 호출할 WAF 모듈 class 명
method: 해당 WAF에서 호출할 Method 명
seq: 호출될 순번
sql_id: WAF를 통해 호출할 경우 조회할 ibatis statement id

type: singleSearch, search, init, save, operate, delete, savensearch
-->

<configuration>
	<!-- 공통 WAF 명령어 -->
	<command id="WAF_COMMON_INIT" type="init">
		<service key="CommonInit" name="com.kbstar.mbc.waf.CommonWAF" element="Common" method="init" seq="1" sql_id=""/>
	</command>
	
	<command id="WAF_COMMON_SEARCH" type="search">
		<service key="CommonSearch" name="com.kbstar.mbc.waf.CommonWAF" element="Common" method="search" seq="1" sql_id=""/>
	</command>
	
	<command id="WAF_COMMON_SAVE" type="save">
		<service key="CommonSave" name="com.kbstar.mbc.waf.CommonWAF" element="Common" method="save" seq="1" sql_id=""/>
	</command>
	
	<!-- 사용자 관리 WAF 명령어 -->
	<command id="WAF_USER_INIT" type="init">
		<service key="UserInit" name="com.kbstar.mbc.waf.UserWAF" element="User" method="init" seq="1" sql_id=""/>
		<service key="UserCodeList" name="com.kbstar.mbc.waf.UserWAF" element="UserCode" method="getCodeList" seq="2" sql_id="common.getCommonCode"/>
	</command>
	
	<command id="WAF_USER_SEARCH" type="search">
		<service key="UserSearch" name="com.kbstar.mbc.waf.UserWAF" element="User" method="search" seq="1" sql_id="user.getUserList"/>
	</command>
	
	<command id="WAF_USER_SAVE" type="save">
		<service key="UserSave" name="com.kbstar.mbc.waf.UserWAF" element="User" method="save" seq="1" sql_id="user.User"/>
	</command>
	
	<command id="WAF_USER_DELETE" type="delete">
		<service key="UserDelete" name="com.kbstar.mbc.waf.UserWAF" element="User" method="delete" seq="1" sql_id="user.deleteUser"/>
	</command>
	
	<!-- 계정 관리 WAF 명령어 -->
	<command id="WAF_ACCOUNT_INIT" type="init">
		<service key="AccountInit" name="com.kbstar.mbc.waf.AccountWAF" element="Account" method="init" seq="1" sql_id=""/>
		<service key="AccountCodeList" name="com.kbstar.mbc.waf.AccountWAF" element="AccountCode" method="getCodeList" seq="2" sql_id="common.getCommonCode"/>
	</command>
	
	<command id="WAF_ACCOUNT_SEARCH" type="search">
		<service key="AccountSearch" name="com.kbstar.mbc.waf.AccountWAF" element="Account" method="search" seq="1" sql_id="account.getAccountList"/>
	</command>
	
	<command id="WAF_ACCOUNT_SAVE" type="save">
		<service key="AccountSave" name="com.kbstar.mbc.waf.AccountWAF" element="Account" method="save" seq="1" sql_id="account.Account"/>
	</command>
	
	<command id="WAF_ACCOUNT_DELETE" type="delete">
		<service key="AccountDelete" name="com.kbstar.mbc.waf.AccountWAF" element="Account" method="delete" seq="1" sql_id="account.deleteAccount"/>
	</command>
	
	<!-- 시스템 코드 관리 WAF 명령어 -->
	<command id="WAF_SYSTEMCODE_INIT" type="init">
		<service key="SystemCodeInit" name="com.kbstar.mbc.waf.SystemCodeWAF" element="SystemCode" method="init" seq="1" sql_id=""/>
	</command>
	
	<command id="WAF_SYSTEMCODE_SEARCH" type="search">
		<service key="SystemCodeSearch" name="com.kbstar.mbc.waf.SystemCodeWAF" element="SystemCode" method="search" seq="1" sql_id="systemcode.getSystemCodeList"/>
	</command>
	
	<command id="WAF_SYSTEMCODE_SAVE" type="save">
		<service key="SystemCodeSave" name="com.kbstar.mbc.waf.SystemCodeWAF" element="SystemCode" method="save" seq="1" sql_id="systemcode.SystemCode"/>
	</command>
	
	<command id="WAF_SYSTEMCODE_DELETE" type="delete">
		<service key="SystemCodeDelete" name="com.kbstar.mbc.waf.SystemCodeWAF" element="SystemCode" method="delete" seq="1" sql_id="systemcode.deleteSystemCode"/>
	</command>
	
	<!-- 환경 설정 관리 WAF 명령어 -->
	<command id="WAF_ENVIRONMENT_INIT" type="init">
		<service key="EnvironmentInit" name="com.kbstar.mbc.waf.EnvironmentWAF" element="Environment" method="init" seq="1" sql_id=""/>
	</command>
	
	<command id="WAF_ENVIRONMENT_SEARCH" type="search">
		<service key="EnvironmentSearch" name="com.kbstar.mbc.waf.EnvironmentWAF" element="Environment" method="search" seq="1" sql_id=""/>
	</command>
	
	<!-- 대시보드 WAF 명령어 -->
	<command id="WAF_DASHBOARD_INIT" type="init">
		<service key="DashboardInit" name="com.kbstar.mbc.waf.DashboardWAF" element="Dashboard" method="init" seq="1" sql_id=""/>
		<service key="DashboardStats" name="com.kbstar.mbc.waf.DashboardWAF" element="DashboardStats" method="getStats" seq="2" sql_id=""/>
	</command>
	
	<command id="WAF_DASHBOARD_SEARCH" type="search">
		<service key="DashboardSearch" name="com.kbstar.mbc.waf.DashboardWAF" element="Dashboard" method="search" seq="1" sql_id=""/>
	</command>
	
	<!-- 보고서 WAF 명령어 -->
	<command id="WAF_REPORT_INIT" type="init">
		<service key="ReportInit" name="com.kbstar.mbc.waf.ReportWAF" element="Report" method="init" seq="1" sql_id=""/>
	</command>
	
	<command id="WAF_REPORT_GENERATE" type="operate">
		<service key="ReportGenerate" name="com.kbstar.mbc.waf.ReportWAF" element="Report" method="generate" seq="1" sql_id=""/>
	</command>
	
	<!-- 시스템 관리 WAF 명령어 -->
	<command id="WAF_SYSTEM_INIT" type="init">
		<service key="SystemInit" name="com.kbstar.mbc.waf.SystemWAF" element="System" method="init" seq="1" sql_id=""/>
	</command>
	
	<command id="WAF_SYSTEM_SEARCH" type="search">
		<service key="SystemSearch" name="com.kbstar.mbc.waf.SystemWAF" element="System" method="search" seq="1" sql_id="system.getSystemLog"/>
	</command>
	
	<!-- 파일 관리 WAF 명령어 -->
	<command id="WAF_FILE_UPLOAD" type="operate">
		<service key="FileUpload" name="com.kbstar.mbc.waf.FileWAF" element="File" method="upload" seq="1" sql_id="common.insertFileInfo"/>
	</command>
	
	<command id="WAF_FILE_DOWNLOAD" type="operate">
		<service key="FileDownload" name="com.kbstar.mbc.waf.FileWAF" element="File" method="download" seq="1" sql_id="common.getFileInfo"/>
	</command>
	
	<command id="WAF_FILE_DELETE" type="delete">
		<service key="FileDelete" name="com.kbstar.mbc.waf.FileWAF" element="File" method="delete" seq="1" sql_id="common.deleteFileInfo"/>
	</command>
	
</configuration> 